{
  "name": "nextjs-spesen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:status": "npx supabase status",
    "db:restart": "npm run db:stop && npm run db:start",
    "db:reset": "npx supabase db reset",
    "db:link": "npx supabase link",
    "db:generate-types": "npx supabase gen types typescript --local --schema public > ./src/lib/supabase/types_db.ts",
